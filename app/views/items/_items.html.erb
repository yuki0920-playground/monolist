<% if items %>
  <div class="row">
    <% items.each do |item| %>
      <div class="col-md-3 col-sm-4 col-xs-12">
        <div class="panel panel-default">
          <div class="panel-heading text-center">
            <img src="<%= item.image_url %>" alt="">
          </div>
        </div>
      </div>
    <% end %>
  </div>
<% end %>